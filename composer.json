{
	"name": "shano/paginator",
	"description": "Pagination library for various list types in PHP.",
	"homepage": "https://github.com/shano/paginator",
	"keywords": ["paginator", "lists", "array", "eloquence"],
	"authors": [
		{
			"name": "Shane Dowling",
			"homepage": "https://github.com/shano"
		}
	],
	"require": {
		"php": ">=7",
		"illuminate/support": "^7.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^7"
	},
	"type": "library",
	"autoload": {
		"psr-4": { "Paginator\\": "library/" }
	},
	"autoload-dev": {
		"psr-4": {"Paginator\\Tests\\": "tests/"}
	}
}
